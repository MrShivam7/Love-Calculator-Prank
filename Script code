<script>
    function copyit(){
        var id = 'cpfinalurl';
        var el = document.getElementById(id);
        var range = document.createRange();
        range.selectNodeContents(el);
        var sel = window.getSelection();
        sel.removeAllRanges();
        sel.addRange(range);
        document.execCommand('copy');
        $('#clp').toggleClass('mt-3 mx-auto w-64 hover');
        return false;
    }
</script>
